{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "SalesData-adf14"
		},
		"ls_DataBricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_DataBricks'"
		},
		"ls_destination_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_destination_adls'"
		},
		"ls_DataBricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1227-071056-ro57iur8"
		},
		"ls_destination_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://salesdatalake14.dfs.core.windows.net/"
		},
		"ls_onPrem_DataSource_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "LAPTOP-1MT0A11L\\SQLEXPRESS"
		},
		"ls_onPrem_DataSource_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "testdbnew"
		},
		"ls_onPrem_DataSource_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vinna"
		},
		"ls_salesdatakeyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://SalesDataKeyVault14.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "SalesData-adf14_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_DataBricks_accessToken": {
						"value": "[parameters('ls_DataBricks_accessToken')]"
					},
					"ls_destination_adls_accountKey": {
						"value": "[parameters('ls_destination_adls_accountKey')]"
					},
					"ls_DataBricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_DataBricks_properties_typeProperties_existingClusterId')]"
					},
					"ls_destination_adls_properties_typeProperties_url": {
						"value": "[parameters('ls_destination_adls_properties_typeProperties_url')]"
					},
					"ls_salesdatakeyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_salesdatakeyvault_properties_typeProperties_baseUrl')]"
					},
					"ls_onPrem_DataSource_properties_typeProperties_server": {
						"value": "[parameters('ls_onPrem_DataSource_properties_typeProperties_server')]"
					},
					"ls_onPrem_DataSource_properties_typeProperties_database": {
						"value": "[parameters('ls_onPrem_DataSource_properties_typeProperties_database')]"
					},
					"ls_onPrem_DataSource_properties_typeProperties_userName": {
						"value": "[parameters('ls_onPrem_DataSource_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}